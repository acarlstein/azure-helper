{
	"name": "azure-helper",
	"displayName": "azure-helper",
	"description": "Helping developers to code in Azure",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:shellLauncher.launch",
		"onCommand:terminalTest.sendText"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "azure-helper.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "shellLauncher.launch",
				"title": "Shell Launcher: Launch"
			},
			{
				"command": "terminalTest.sendText",
				"title": "Terminal API: Send Text"
			},
			{
				"command": "extension.terminalCapture.runCapture",
				"title": "Terminal: Capture"
			}
		],
		"configuration": {
			"title": "Shell Launcher and Terminal Capture configuration",
			"properties": {
				"terminalCapture.enable": {
					"type": "boolean",
					"description": "If false, disable the terminal capture extension",
					"default": true
				},
				"terminalCapture.useClipboard": {
					"type": "boolean",
					"description": "If false, use the cache mode that tracks all history",
					"default": true
				},
				"shellLauncher.shells.osx": {
					"type": "array",
					"description": "A list of shell configurations for macOS",
					"default": [
						{
							"shell": "/bin/bash",
							"label": "bash"
						},
						{
							"shell": "/bin/zsh",
							"label": "zsh"
						},
						{
							"shell": "zsh",
							"args": [
								"-l"
							],
							"label": "zsh login shell"
						},
						{
							"shell": "bash",
							"args": [
								"-l"
							],
							"label": "bash login shell"
						}
					],
					"items": {
						"shell": "string",
						"args": "string[]",
						"label": "string",
						"launchName": "string",
						"cwd": "string",
						"env": "string[]"
					}
				},
				"shellLauncher.shells.linux": {
					"type": "array",
					"description": "A list of shell configurations for Linux",
					"default": [
						{
							"shell": "/bin/bash",
							"label": "bash"
						},
						{
							"shell": "/bin/zsh",
							"label": "zsh"
						},
						{
							"shell": "zsh",
							"args": [
								"-l"
							],
							"label": "zsh login shell"
						},
						{
							"shell": "bash",
							"args": [
								"-l"
							],
							"label": "bash login shell"
						}
					],
					"items": {
						"shell": "string",
						"args": "string[]",
						"label": "string",
						"launchName": "string",
						"cwd": "string",
						"env": "string[]"
					}
				},
				"shellLauncher.shells.windows": {
					"type": "array",
					"description": "A list of shell configurations for Windows",
					"default": [
						{
							"shell": "C:\\Windows\\System32\\cmd.exe",
							"label": "cmd"
						},
						{
							"shell": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
							"label": "PowerShell"
						},
						{
							"shell": "C:\\Program Files\\Git\\bin\\bash.exe",
							"label": "Git bash"
						},
						{
							"shell": "C:\\Windows\\System32\\bash.exe",
							"label": "WSL Bash"
						}
					],
					"items": {
						"shell": "string",
						"args": "string[]",
						"label": "string",
						"launchName": "string",
						"cwd": "string",
						"env": "string[]"
					}
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"path": "^0.12.7"
	},
	"trace": true
}